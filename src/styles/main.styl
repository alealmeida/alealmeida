section
	justify-content: center
	display: flex
	flex-direction: column

.container
	.pack-page
		position relative
		display flex
		min-height 100vh
		.tween-one, .tween
			position fixed

.tween
	width auto
	height auto
	left 10vw
	width 100vw
	flex-direction column
	top 4em
	text-align left
	justify-items flex-start
	align-items flex-start
	position: absolute

.tween-one
	overflow-y auto
	flex-direction column
	width 100vw
	min-height 100vh
	opacity 1
	text-align left
	justify-items flex-start
	align-items flex-start
	position: absolute
	top 4em
	left 10vw

	& .rc-collapse
		display flex
		position absolute
		background-color transparent
		border none

.cabecalho
	display: flex;
	flex-direction: column;
	width: 80vw
	height 90vh
	top 5vh

	h1
		font-family: neue-kabel,sans-serif;
		font-weight: 800;
		font-style: normal;
		position: fixed;
		font-size: calc(4.5em + 520 * ((100vw + 100vh) / 2 / 1120));
		margin: 0;
		padding: 0;
		line-height: 1em;
		text-align: center;
		display: flex;
		flex-direction: column;
		width: 10vw;
		width: 100vw;
		justify-content: flex-end;
		align-self: center;
		align-items: flex-start;
