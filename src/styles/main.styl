// link css
.logo
	height 58px
	font-size calc( 10px + 5 * ( ( 100vw + 100vh ) / 2 / 1120 ) )
	font-weight 500
	letter-spacing 0.05vw
	line-height 56px
	display flex
	align-items left
	justify-content center
	flex-direction column
	margin 0 0 0 calc( 1em + 2vw )
	position fixed
	top 0
	left 10vw

.nav
	display flex
	align-items flex-start
	justify-content flex-start
	top 0
	left 10vw
	position fixed

	& .nav-wap
		float right
		padding-right 200px
		// 

	& .nav-list
		width 100px
		float left
		line-height 58px
		text-align center
		cursor pointer
		visibility hidden

	& .logo
		visibility visible

	& .nav-bar
		width 100px
		position absolute
		top 58px
		left 0
		height 2px
		transition left 0.3s

