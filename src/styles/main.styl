.logo{
  height:58px;
  font-size:calc(10px + 5 * ((100vw + 100vh)/2 / 1120));
  font-weight: 500;
  letter-spacing: 0.05vw;
  line-height:56px;
  display: flex;
  align-items: left ;
  justify-content: center;
  flex-direction: column;
  margin: 0 0 0 calc(1em + 2vw);
}
.nav {
  position: fixed;

  display: flex;
  z-index: 8;
  top: 0;
  & .nav-wap{
    float: right;
    padding-right:200px;
    // 
  }
  & .nav-list {
    width: 100px;
    height: 100%;
    float: left;
    line-height: 58px;
    text-align: center;
    cursor: pointer;
    visibility hidden;
    &:hover, &.active {
    }
  }
  & .logo {
    visibility: visible;  
  }
  & .nav-bar {
    width: 100px;
    position: absolute;
    top: 58px;
    left: 0;
    height: 2px;
    transition: left .3s;
  }
}
