:root
	--white #fff
	--grey #f1f4f8b0
	--dark-grey #6b7c93
	--green #24b47e
	--teal #4F96CE
	--blue #6772e5
	--dark-blue #4F3EF5
	--spacer 28px

html,
body
	margin 0
	padding 0
	width 100%
	height 100%

html
	height 100%

body
	// font-family 
	font-family 'Montserrat'
	font-style normal
	font-weight 100
	overflow-x hidden
	overflow-y scroll
	perspective 1px
	transform-style preserve-3d
	/* Support for all WebKit browsers. */
	-webkit-font-smoothing: auto;
	/* Support for Safari and Chrome. */
	text-rendering: optimizeLegibility;

	/* Support for Firefox. */
	-moz-osx-font-smoothing: grayscale;

a
	color #000000

a:hover
	color #ff0000

section
	height 100vh
	width 100vw

.container
	scroll-snap-type y mandatory
	height 100%
	overflow scroll

.container
