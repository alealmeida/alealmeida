.modal {
  position: fixed;
  top: 0;
  left: 0;
  width: 100vw;
  height: 100vh;
  z-index: 1050;
  overflow-y: scroll;

  .close {
    position: fixed; 
    top: 1em;
    right: 1em;
}
}


.container {

.pack-page{

  position: relative;
  display: flex;
  min-height: 100vh;
  background-color: #f0f0f0;
  
  .tween-one{
    overflow-y: auto;
    flex-direction: column;
    width: 80vw;
    min-height: 100vh;
    opacity: 0;
    text-align: left;
    justify-items: flex-start;
    align-items: flex-start;
    top: 4em;
    position: fixed;

    div {
      
      text-align:left;
      font-size:calc(12px + 10 * ((100vw + 100vh)/2 / 1120));
      line-height: calc(20px + 10 * ((100vw + 100vh)/2 / 1120));
    }
    & .rc-collapse {
      display flex;
      position absolute;
      background-color: transparent;
      border: 0;
    }
  }
}
}
.tween-one h2{
      text-align:left;
      color:#444;
      font-size:calc(20px + 40 * ((100vw + 100vh)/2 / 1120));
    }
