.vertical
	section
		&#intro
			height 100vh
			header
				width: 100vw
				flex_column(center, center)
				dist(0,130)
				height 100%
				>label
					text-align: center
					font-size: 1.5em
					position relative
					dist(0,-100)
				h3,p
					font(500, 26, neue-h, 0)
					dist(0, 0)
					text-align: center
					align-items: center
					span
						line_letter(18, 4)
						font(300, 32, neue-h, 0)
						position relative
						left 15vw
						text-align: center
					i
						float none
				>label, h3, p
					width: calculo(1em,410, 170)
						
		&#page1
			width: 100%
			reset()
			header
				reset()
				h2
					font(900, 230, neue-h, 0)
					width: 100%
					line_letter(-20, -9)
					dist(30, 80)
			article
				width: 100vw
				flex_column(center, center)
				height calculo(0.4em, 400, 160)
				left 0
				p
					font(400, 10, neue-h, 46)
					dist(0, 20)
					line_letter(15, 1)
					text-align: center
					position relative
					flex_column(center, center)
		
	.main
		margin 0
		position relative
		display: flex
		width: auto
		flex_column(center, center)
		.cases
			mw = calc(1.2em + 4vw)
			sub = calc(100vw - calc(1.2em + 4vw)*2)
			&#pop
			&#natura
			&#mars
			&#caixa
			&#dkids
				width: sub
				margin 0 mw mw
				flex_column(center, center)
				header
					width: 100%
					height: 100%
					overflow none
					dist(0,0)
					flex_column(center, center)
					> div
						width: 70%
						height: 100%
						flex_column(center, center)
						position relative
					label
						font-size: 76vmin
						top: 10vmin
					h4
						font(900, 65, neue-h, 0)
						font-weight: 700
						line_letter(40, 7)
						left auto
						top auto 
						position relative
						reset()
						text-indent: calculo(0.01em,2, 2)
					p
						font(400, 23, neue-h, 0)
						width auto
						line_letter(12, 2)
						left auto
						top auto 
						position relative
						reset()
					h4,p
						width 60%
						text-align: center
				article
					display: none
			#natura
				h = 60vh
				height h
				a = 272
				b = 2
				vm = 17vmin
				c = a + 56
				header
					clip: rect(0, 100vw, h, 0)
					figure
						bottom: calculo(vm,b, 40)
				article
					img
						width: 45vw
						height auto
						top 140vh
						left auto
						filter: blur(1px)
						opacity 1
			#pop
				h = 60vh
				height h
				a = 272
				b = 2
				vm = 17vmin
				c = a + 56
				header
					clip: rect(0, 100vw, h, 0)
					figure
						bottom: calculo(vm,b, 40)
				article
					img
						width: 45vw
						height auto
						top 140vh
						left auto
						filter: blur(1px)
						opacity 1
			#pop
				h = 60vh
				height h
				a = 272
				b = 2
				vm = 17vmin
				c = a + 56
				header
					clip: rect(0, 100vw, h, 0)
					figure
						bottom: calculo(vm,b, 40)
				article
					img
						width: 45vw
						height auto
						top 140vh
						left auto
						filter: blur(1px)
						opacity 1
			#mars
				h = 60vh
				height h
				a = 272
				b = 2
				vm = 17vmin
				c = a + 56
				header
					clip: rect(0, 100vw, h, 0)
					figure
						bottom: calculo(vm,b, 40)
				article
					img
						height 160vh
						left -10vmin
						top -110vmin
						filter: blur(1.5px)
						opacity 0.8
			#caixa
				h = 60vh
				height h
				a = 272
				b = 2
				vm = 17vmin
				c = a + 56
				header
					clip: rect(0, 100vw, h, 0)
					figure
						bottom: calculo(vm,b, 40)
				article
					flex_column(flex-start,center)
					padding 0
					margin: 0
					width: 100vw
					img
						width: 95vw
						height auto
						top 110vh
						left auto
						filter: blur(1px)
						opacity 1
			#dkids
				h = 60vh
				height h
				a = 272
				b = 2
				vm = 17vmin
				c = a + 56
				header
					clip: rect(0, 100vw, h, 0)
					figure
						bottom: calculo(vm,b, 40)
				article
					flex_column(flex-start,center)
					padding 0
					margin: 0
					width: 100vw
					img
						width: 95vw
						height auto
						top 110vh
						left auto
						filter: blur(1px)
						opacity 1
	.interna
		#Scroll-Pack
			>div
				dist(0,0)
				.barra_topo
					min-height: 3.8em
					h4
						font(600, 16, neue-h, 50)
						dist(188,0)
				.nav-wap
					top 0	
					.logo
						position: fixed
						z-index 9999
				.callback
					.logo
						dist(54,0)
						font(600, 18, neue-h, 45)
						min-height: 3.1em
					.back
						left 2vmin
						margin-top 0.1vmin
						min-width: 14vmin
						z-index 9999
						min-height: 2.6em;
		&.cases
			a = 30
			h = 90vh
			#hero
				height 90vh !important
				clip: rect(0, 100vw, h, 0)
				.titulo
					text-align: center;
					text-indent: 4vmin;
					h3
						font(900, 95, neue-h, 45)
						margin: calculo(1em,0, 70) 0 0
						line_letter(20,20)
			&#caixa
				#hero
					img
						dist(-810,-100)
						width calculo(1em,400, 68*a)
				article
					.caixa_001
						width: calculo(1em,400, 6*a)
						dist(0,50)
					.caixa_003
						width: calculo(1em,180, 8*a)
					.caixa_005
						width: calculo(1em,180, 8*a)
					.caixa_016
						width: calculo(1em,180, 5*a)
					.caixa_006
						width: calculo(1em,180, 8*a)
					.caixa_full
						width: calculo(1em,180, 11*a)
					.caixa_parts_mob
						width: calculo(1em,180,11*a)
						box-shadow: 1px 0px 5vmin -12px rgba(0,0,0,0.1)
			&#natura
				#hero
					img
						dist(70,200)
						width calculo(1em,400, 6*a)
				article
					.natura_001
						width: calculo(1em,400, 3*a)
						dist(0,10)
					.natura_002
						width: calculo(1em,200, 2*a)
					.natura_003
						width: calculo(1em,200, 7*a)
					.natura_003_2
						width: calculo(1em,200, 7*a)
					.natura_004
						width: calculo(1em,200, 9*a)
					.natura_005
						width: calculo(1em,200, 15*a)
					.natura_006
						width: calculo(1em,200, 7*a)
			&#mars
				#hero
					img
						dist(-1210,100)
						height calculo(1em,400, 25*a)
				article
					.mms_001
						width: calculo(1em,400, 5*a)
						dist(105,48)
					.mms_002
						width: calculo(1em,400, 4*a)
						dist(0,58)
						box-shadow: 1px 0px 8vmin -12px rgba(0,0,0,0.1)
					.mms_003
						width: calculo(1em,400, 4*a)
						dist(0,-50)
						box-shadow: 1px 0px 10vmin -12px rgba(0,0,0,0.4)
					.mms_004
						width: calculo(1em,180, 5*a)
						box-shadow: 1px 0px 10vmin -12px rgba(0,0,0,0.2)
			&#pop
				#hero
					img
						dist(-460,-140)
						width calculo(1em,400, 28*a)
				article
					.pop_001
						width: calculo(1em,400, 15*a)
						dist(175,48)
					.pop_002
						width: calculo(1em,350, 14*a)
						dist(-165,0)
						
					.pop_003
						width: calculo(1em,320, 15*a)
						dist(146,-40)
						box-shadow: 1px 0px 10vmin -12px rgba(0,0,0,0.1)
					.pop_full
						width: calculo(1em,180, 10*a)
						box-shadow: 1px 0px 10vmin -12px rgba(0,0,0,0.1)
			&#dkids
				#hero
					img
						dist(-30,130)
						width calculo(1em,400, 18*a)
				article
					.dkids_001
						dist(-20,10)
						width: calculo(1em,400, 17*a)
						margin-bottom: calculo(1em,400, -21*a)
					.dkids_003
						width: calculo(1em,400, 2*a)
					.dkids_004
						width: calculo(1em,400, 2*a)
						margin: calculo(1em,0, 5) 0
					.dkids_005
						width: calculo(1em,400, 3*a)
						box-shadow: 1px 0px 5vmin -12px rgba(0,0,0,0.2)
						margin: calculo(1em,0, 10) 0
					.dkids_full
						width: calculo(1em,180, 3*a)
			.destaque
				header
					width: 94%
					margin:calculo(3vmin,0, 0) 0 calculo(3vmin,0, 0)
					h4
						font(600, 30, neue-h, 40)
						line_letter(16,1)
					h5
						font(500, 28, neue-h, 40)
						line_letter(16,1)
					label
						font(500, 2, neue-h, 40)
						line_letter(16,-0.5)
						word-spacing: 0.1em
					.side-content
						width: calculo(1em, 0, 40)
					.main-content
						width: calculo(1em, 0, 470)
						dist(190,0)
						p
							width: 60%
							font-size: 0.78em
							line_letter(14, 1)
		
		.image
			&.margin
				margin 0 calculo(1em, 0, 25) 
				width 88vw
		.content
			w = 90vw
			width w
			dist(10,0)
			a = 30
			header,
			article
				width w
			header
				h5,
				h6
					width w - 3
					line_letter(12, 1)
					dist(0,0)
				h5
					font-size: 1.8em

				h6
					font-size: 1.2em
					line_letter(28, 0.1)
					margin: 10vmin 0
					span
						line_letter(28, 0.1)
			article
				p	
					width w - 3
					font-size: 0.9em
					line_letter(28, 0.1)
					dist(0,0)
					smoo()
				blockquote
					font-size: 1em
					line_letter(14, 2)
					width calculo(44vmin,10, 45)
					padding-left: 1em
					dist(20,0)
				img
					width calculo(75vmin,10, 150)
					position relative
		&.content,
		&.image
			margin: 0
			position relative
			z-index: 3
			height auto
			display: flex
			w =  64vmin
			.bxx_margin
				margin: calculo(1em,0, 80) 0 calculo(1em, 0,10)
			.bx_margin
				margin: calculo(1em,0, 60) 0 calculo(1em, 0,10)
			.b_margin
				margin: calculo(1em,0, 40) 0 calculo(1em, 0,5)
			.mx_margin
				margin: calculo(1em,0, 30) 0 calculo(1em, 0,5)
			.m_margin
				margin: calculo(1em,0, 15) 0 calculo(1em, 0,2)
			.s_margin
				margin: calculo(1em,0, 5) 0 calculo(1em, 0,5)
			.sx_margin
				margin: calculo(1em,0, 2) 0 calculo(1em, 0,2)
				
		&#footer
			header
				h3
					font(300, 50, neue-h, 70)
					line_letter(3, 3)
			article
				flex-direction: column
				padding-bottom: 13vh
				p
					width: 100%
					&.right
						dist(0,40)
						font(300,-7.5, neue-t, 70)
						line_letter(23, 3)
