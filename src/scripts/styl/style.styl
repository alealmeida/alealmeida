@keyframes rect
	from
		clip: rect(0, 100vw, 0, 0)
	to
		clip: rect(0, 100vw, 100%, 0)


animate_clip(time)
	animation: time 0s cubic-bezier(0.770, 0.000, 0.175, 1.000) 1 alternate rect



