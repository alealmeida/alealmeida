w = 100vw
h = 100vh
calculo(em a, b)
	w = 80vw
	h = 20vh
	"calc(%s + (%s + %s) * (%s + %s) / 1000)" % (em a b w h)


dist(left, top)
	left 'calc(%s * ((80vw + 20vh)  / 1000))' % (left w h)
	top 'calc(%s * ((34vw + 66vh) / 1000))' % (top w h)
dist_invert(right, bottom)
	right 'calc(%s * ((80vw + 20vh)  / 1000))' % (right w h)
	bottom 'calc(%s * ((34vw + 66vh) / 1000))' % (bottom w h)

resolucao(width, height)
	width 'calc(1em + %s * ((%s + %s) / 1000))' % (width w h)
	height 'calc(1em + %s * ((%s + %s) / 1000))' % (height w h)

min(n)
	min-width n
	min-height n
max(n)
	max-width n
	max-height n

font(weight, size, family, wid)
	font-weight weight
	font-size 'calc(1em + %s * ((76vw + 24vh) / 1000))' % (size)
	font-family family
	width 'calc(%s * ((86vw + 14vh) / 100))' % (wid)

line_letter(lines, letter)
	line-height 'calc(1em + %s * ((76vw + 24vh) / 1000))' % (lines)
	letter-spacing 'calc(.01em + %s * ((76vw + 24vh) / 1000))' % (letter)

flex_column(justify, align)
	display flex
	flex-direction: column
	justify-content: justify
	align-items: align

cubic(num)
	transition: all num cubic-bezier(0,.49,.85,.91) -0.2s
smoo()
	-webkit-font-smoothing antialiased
	-moz-osx-font-smoothing grayscale
nosmoo()
	-webkit-font-smoothing auto
	-moz-osx-font-smoothing grayscale
reset()
	margin 0
	padding 0

neue-h = neue-haas-grotesk-display, sans-serif
neue-k = neue-kabel, sans-serif
neue-t = neue-haas-grotesk-text, sans-serif
roboto = 'roboto', sans-serif
w-sans = 'work-sans', sans-serif
material = Material Icons


